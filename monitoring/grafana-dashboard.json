{
  "dashboard": {
    "id": null,
    "title": "Local RAG Assistant",
    "tags": ["rag", "ai"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Query Response Time",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(rag_query_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Total Queries",
        "type": "stat",
        "targets": [
          {
            "expr": "rag_queries_total",
            "legendFormat": "Total Queries"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Documents Processed",
        "type": "stat",
        "targets": [
          {
            "expr": "rag_documents_total",
            "legendFormat": "Documents"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 18,
          "y": 0
        }
      },
      {
        "id": 4,
        "title": "Cache Hit Rate",
        "type": "gauge",
        "targets": [
          {
            "expr": "rate(rag_embedding_cache_hits[5m]) / rate(rag_queries_total[5m]) * 100",
            "legendFormat": "Cache Hit Rate %"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "5s"
  }
} 